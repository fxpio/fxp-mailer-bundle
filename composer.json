{
    "name": "sonatra/mailer-bundle",
    "type": "symfony-bundle",
    "description": "A manager of HTML mail template stored in multiple systems and sent with different transports",
    "keywords": ["email", "template", "html", "mail", "manager", "switmailer", "doctrine", "twig", "bundle"],
    "homepage": "https://github.com/sonatra/sonatra-mailer-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0.0",
        "symfony/framework-bundle": "^3.0.0",
        "symfony/yaml": "^3.0.0",
        "sonatra/mailer": "^1.0.0"
    },
    "require-dev": {
        "swiftmailer/swiftmailer": "^5.4.0",
        "doctrine/doctrine-bundle": "^1.6.0",
        "doctrine/orm": "^2.5.0"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\MailerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
