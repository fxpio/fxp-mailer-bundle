{
    "name": "fxp/mailer-bundle",
    "type": "symfony-bundle",
    "description": "A manager of HTML mail template stored in multiple systems and sent with different transports",
    "keywords": ["email", "template", "html", "mail", "manager", "switmailer", "doctrine", "twig", "bundle"],
    "homepage": "https://github.com/fxpio/fxp-mailer-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "symfony/yaml": "^3.4.0|^4.0.0",
        "fxp/mailer": "^1.0.0"
    },
    "require-dev": {
        "swiftmailer/swiftmailer": "^6.0.0",
        "doctrine/doctrine-bundle": "^1.8.0",
        "doctrine/orm": "^2.6.0"
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\MailerBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
